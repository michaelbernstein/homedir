#!/usr/bin/python

import os
import sys
import urllib

cmd = '/usr/bin/gnome-www-browser'
arg = urllib.quote(' '.join(sys.argv[1:])) if len(sys.argv) > 1 else ''
url = 'http://en.wikipedia.org/wiki/Special:Search?search=%s&go=Go' % arg
args = [cmd, '-new-tab', url]

os.execv(cmd, args)

sys.exit(1)
